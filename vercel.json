{
    "redirects": [
      { "source": "/", "destination": "/index.html" },
      { "source": "/agents", "destination": "/", "permanent": false },
      {
        "source": "/:path((?!uk/).*)",
        "has": [
          {
            "type": "header",
            "key": "x-vercel-ip-country",
            "value": "GB"
          }
        ],
        "destination": "/uk/:path*",
        "permanent": false
      }
    ]
  }